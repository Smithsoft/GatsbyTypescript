{
  "name": "Gatsby-Bootcamp-Typescript",
  "private": true,
  "description": "Learning Gatsby with Typescript",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -p 8001",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "kate": "cal",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^2.23.3",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sass": "^2.3.1",
    "gatsby-plugin-typescript": "^2.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "@types/node-sass": "^4.11.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react-inlinesvg": "^1.2.0",
    "svg-inline-loader": "^0.8.2",
    "typed-scss-modules": "^1.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --ext **/*.d.ts --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Smithsoft/GatsbyTypescript"
  },
  "bugs": {
    "url": "https://github.com/Smithsoft/GatsbyTypescript/issues"
  }
}
